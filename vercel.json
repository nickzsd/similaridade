{
    "version": 2,
    "builds": [
      {
        "src": "Controller/similarity.py", 
        "use": "@vercel/python",
        "config": {          
          "pythonVersion": "3.12"          
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Controller/similarity.py"
      }
    ]
  }